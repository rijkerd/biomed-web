  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "contributors": [
    {
      "name": "Ricardo Aggrey",
      "email": "richardaggrey7@gmail.com",
      "url": "https://github.com/rijkerd"
