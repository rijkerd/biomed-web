  "engines": {
    "node": ">=8.11.1",
    "npm": ">=5.6.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ],
    ".{css,scss}": [
      "prettier --write"
    ],
    "*.json": [
